<p style="color: green"><%= notice %></p>

<%= render @album_track %>

<div>
  <%= link_to "Back to album tracks", album_tracks_path %>
  <% if policy(@album_track).update? %>
    <%= link_to 'Edit', edit_album_track_path(@album_track) %>
  <% end %>

  <% if policy(@album_track).destroy? %>
    <%= link_to 'Delete', @album_track, method: :delete %>
  <% end %>
</div>
